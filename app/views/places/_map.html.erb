<script>

  function initMap() {
    var myLatLng = {lat: <%= place.latitude %>, lng: <%= place.longitude %>};

    var map = new google.maps.Map(document.getElementById('map'), {
      zoom: 13,
      center: myLatLng
    });

    var marker = new google.maps.Marker({
      position: myLatLng,
      map: map,
      title: 'Hello World!'
    });
  }
</script>

<script async defer
src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBpdYH6MYNxbaIrV6Vky7_hIcGLlxphAvo&callback=initMap">
</script>

<div id="map"></div>
